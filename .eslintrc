{
  "extends": "airbnb",  
  //"parser": "babel-eslint",
  "globals": {
    "jQuery": true,
    "$": true,
    "angular": true,
    "moment": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    //http://eslint.org/docs/rules/
    "linebreak-style": 0,
    "func-names": 0,
    "indent" : 0,
    "space-before-function-paren": 0,
    "no-unused-vars": 1,
    "comma-dangle": 0, //数组最后一项必须有逗号
    "import/no-extraneous-dependencies": 0,
    "prefer-const": 0,
    "react/jsx-indent": 0,
    "quote-props": 0,

    "no-console": 0,
    "no-var": 0,
    "no-new": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": [0],
    "prefer-template": 0,
    "object-shorthand": [0],
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 0,
    "no-param-reassign": 0,
    "max-len": ["error", {
      "code": 200,
      "tabWidth": 2,
      "comments": 200,
      "ignoreComments": false,
      "ignoreTrailingComments": false,
      "ignoreUrls": true
    }],
    "global-require": 0,
    "no-alert": 0,
    'no-restricted-syntax': [
      2,
      'LabeledStatement',
      'WithStatement',
    ]
  }
}